{
	"annotation": {
		"name": "Annotate reads",
		"path": "pipelines/demux_map",
		"config_file": "config.yaml",
		"requires": [
			{
				"file": "references.fasta",
				"config_key": "references_file"
			}
		]
	},
	"processing": [
		{
			"name": "Build consensus (ARTIC pipeline)",
			"path": "pipelines/build_consensus_artic",
			"config": "min_length=100 max_length=10000"
		},
		{
			"name": "Export reads",
      "path": "pipelines/bin_to_fastq",
      "config_file": false,
      "run_per_sample": true,
      "options": {
        "min_length": true,
        "max_length": true,
        "sample_name": true,
        "barcodes": true,
        "basecalled_path": true,
        "annotated_path": true,
        "output_path": true
      }
		}
	]
}
